<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bzw.entity.AuthorityEntity" table="authority" schema="" catalog="bzw">
        <id name="authorityId" column="authority_id"/>
        <property name="authorityCode" column="authority_code"/>
        <property name="authorityName" column="authority_name"/>
        <property name="parentid" column="parentid"/>
        <property name="memo" column="memo"/>
    </class>
</hibernate-mapping>